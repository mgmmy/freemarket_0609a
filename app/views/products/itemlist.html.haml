=render partial: "header"

.mid-main-wrapper.clearfix
  .all-list-wrapper
    %ul.navi-item-wrapper.clearfix
      %li
        =link_to 'ショルダーバッグ','#', class:'sml-category'
      %li
        =link_to 'トートバッグ','#', class:'sml-category'
      %li
        =link_to 'ハンドバッグ','#', class:'sml-category'
      %li
        =link_to 'リュック/バックパック','#', class:'sml-category'
      %li
        =link_to 'ポーチ/バニティ','#', class:'sml-category'
      %li
        =link_to 'ショップ袋','#', class:'sml-category'
      %li
        =link_to 'ボディーバッグ/ウエストバッグ','#', class:'sml-category'          
      %li
        =link_to 'かごバッグ','#', class:'sml-category'
      %li
        =link_to '旅行用バッグ/キャリーバッグ','#', class:'sml-category'
          
