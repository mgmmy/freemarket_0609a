      %ul.item-list-page
        %li.item-list-page_prev
          %ul
            %li.progress-box
              %a{href: "#"}
                %i.fa.fa-angle-double-left.icon-double
            %li.progress-box
              %a{href: "#"}
                %i.fa.fa-chevron-left.icon-single
        %li.item-list-page_num
          %ul
            %li.page-btn.active
              %a{href: "#"}
              1
            %li.page-btn.box
              %a{href: "#"}
              2
            %li.page-btn.box
              %a{href: "#"}
              3
            %li.page-btn.box
              %a{href: "#"}
              4
            %li.page-btn.box
              %a{href: "#"}
              5
        %li.item-list-page_next
          %ul
            %li.progress-box
              %a{href: "#"}
                %i.fa.fa-chevron-right.icon-single
            %li.progress-box
              %a{href: "#"}
                %i.fa.fa-angle-double-right.icon-double

      .item-list-footer
        .item-list-name_box
          %h3.item-list-name
            バッグ（レディース）

